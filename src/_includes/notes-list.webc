<div webc:for="note in notes.reverse()" webc:nokeep>
    <ul>
        <li><p class="h-entry">
            <a class="u-url p-name" :href="notes[note].url"><time :@value="notes[note].date"
                 @machine-format="yyyy-LL-dd"
                 @format="d LLLL yyyy"></time>
            </a>
        </p>
        <p @raw="notes[note].content"></p>
    </li>
</ul>
</div>